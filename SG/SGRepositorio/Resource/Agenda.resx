  <data name="AgendaServicoAdd" xml:space="preserve">
    <value>insert into AgendaServico (IdAgenda,IdServico) values (@IdAgenda,@IdServico)</value>
  </data>
  <data name="AgendaServicoDel" xml:space="preserve">
    <value>delete from agendaservico where idagenda = @IdAgenda</value>
  </data>
  <data name="Editar" xml:space="preserve">
    <value>UPDATE [dbo].[Agenda]
   SET [IdProfissional] = @IdProfissional
      ,[IdCliente] = @IdCliente
      ,[Data] = @Data
      ,[HoraInicial] = @HoraInicial
      ,[HoraFinal] = @HoraFinal
      ,[Situacao] = @Situacao
      ,[Complemento] = @Complemento
 WHERE Agenda.IdAgenda = @IdAgenda</value>
  </data>
  <data name="Inserir" xml:space="preserve">
    <value>INSERT INTO [dbo].[Agenda]
           ([IdProfissional]
           ,[IdCliente]
           ,[Data]
           ,[HoraInicial]
           ,[HoraFinal]
           ,[Situacao]
           ,[Complemento])
     VALUES
           (@IdProfissional
           ,@IdCliente
           ,@Data
           ,@HoraInicial
           ,@HoraFinal
           ,@Situacao
           ,@Complemento); select SCOPE_IDENTITY();</value>
  </data>
  <data name="InserirOcorerncia" xml:space="preserve">
    <value>insert into AgendaOcorrencia (IdUsuario,IdAgenda,DataHora,Motivo) values (@IdUsuario,@IdAgenda,@DataHora,@Motivo)</value>
  </data>
  <data name="Recuperar" xml:space="preserve">
    <value>Select	A.IdAgenda as AIdAgenda,
		A.Data as AData,
		A.HoraFinal as AHoraFinal,
		A.HoraInicial as AHoraInicial,
		A.Situacao as ASituacao,
		A.Complemento as AComplemento,
		p.IdProfissional as PIdProfissional,
		P.Nome as PNome,
		P.Telefone as PTelefone,
		p.Celular as PCelular,
		C.IdCliente as CIdCliente,
		C.Nome as CNome,
		c.Telefone as CTelefone,
		c.Celular as CCelular
from Agenda A
inner join Profissional p ON a.IdProfissional = p.IdProfissional
inner join Cliente c ON A.IdCliente = c.IdCliente
where A.IdAgenda = @IdAgenda</value>
  </data>
  <data name="RecuperarLista" xml:space="preserve">
    <value>Select	A.IdAgenda as AIdAgenda,
		A.Data as AData,
		A.HoraFinal as AHoraFinal,
		A.HoraInicial as AHoraInicial,
		A.Situacao as ASituacao,
		A.Complemento as AComplemento,
		p.IdProfissional as PIdProfissional,
		P.Nome as PNome,
		P.Telefone as PTelefone,
		p.Celular as PCelular,
		C.IdCliente as CIdCliente,
		C.Nome as CNome,
		c.Telefone as CTelefone,
		c.Celular as CCelular
from Agenda A
inner join Profissional p ON a.IdProfissional = p.IdProfissional
inner join Cliente c ON A.IdCliente = c.IdCliente
where A.Data = @Data</value>
  </data>
  <data name="RecuperarListaOcorrencias" xml:space="preserve">
    <value>select	Agenda.data as AData,
Agenda.HoraInicial as AHoraInicial,
agenda.HoraFinal as AHoraFinal,
agenda.idagenda as AIdAgenda, 

		profissional.nome as PNome,
		profissional.IdProfissional as PIdProfissional, 
		
		cliente.nome as CNome, 
		cliente.idcliente as CIdCliente,
		
		agendaocorrencia.Motivo as OMotivo,
		agendaocorrencia.IdOcorrencia as OIdOcorrencia,
		agendaocorrencia.DataHora as ODataHora 
		
from agendaocorrencia
inner join agenda on agenda.idagenda = agendaocorrencia.idagenda
inner join cliente on cliente.idcliente = agenda.idcliente
inner join profissional on profissional.idprofissional = agenda.idprofissional
where agenda.data = @Data</value>
